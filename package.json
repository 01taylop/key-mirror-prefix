{
  "name": "key-mirror-prefix",
  "description": "A simple utility for creating an object with values equal to its keys, with a prefix.",
  "version": "1.0.0",
  "type": "module",
  "main": "./lib/index.cjs",
  "exports": {
    ".": {
      "require": "./lib/index.cjs",
      "import": "./lib/index.mjs"
    }
  },
  "files": [
    "lib",
    "README.md"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "rimraf lib && babel src -d lib --out-file-extension .cjs && cp src/index.mjs lib/",
    "publishLib": "yarn test --coverage && yarn build && npm publish",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/cli": "7.17.10",
    "@babel/core": "7.17.10",
    "@babel/plugin-proposal-optional-chaining": "7.16.7",
    "@babel/plugin-transform-modules-commonjs": "7.17.9",
    "@babel/preset-env": "7.17.10",
    "babel-jest": "28.1.0",
    "jest": "28.1.0",
    "rimraf": "3.0.2"
  },
  "author": "Patrick Taylor <hello@patricktaylor.dev>",
  "repository": {
    "type": "git",
    "url": "https://github.com/01taylop/key-mirror-prefix"
  },
  "keywords": [
    "key-mirror",
    "key-mirror-prefix"
  ],
  "license": "ISC"
}
