{
  "name": "keymirrorprefix",
  "description": "A utility for prefixing mirrored key-value pairs in JavaScript objects.",
  "repository": {
    "type": "git",
    "url": "https://github.com/01taylop/key-mirror-prefix"
  },
  "version": "1.0.0",
  "type": "module",
  "main": "./lib/index.js",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/index.cjs"
    }
  },
  "engines": {
    "node": ">=12.8.0"
  },
  "files": [
    "lib",
    "README.md"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "rimraf lib && babel src -d lib --out-file-extension .cjs && cp src/index.js lib/",
    "publishLib": "yarn test --coverage && yarn build && npm publish",
    "test": "jest",
    "test:build": "jest --config jest.config.cjs.js"
  },
  "devDependencies": {
    "@babel/cli": "7.24.6",
    "@babel/core": "7.24.6",
    "@babel/preset-env": "7.24.6",
    "jest": "29.7.0",
    "rimraf": "5.0.7"
  },
  "author": "Patrick Taylor <hello@patricktaylor.dev>",
  "keywords": [
    "key-mirror",
    "key-mirror-prefix"
  ]
}
